let fib = fun [n] do
  let fib_help = fun [a, b, n]
    if n > 0 then fib_help(b, a+b, n-1) else a end
  return fib_help(0, 1, n)
end

println(fib(20))

let Obj = {}
Obj.meta = {}
Obj.meta.__get = fun [thing, key] print("There isn't a key: ", key, "\n")

Obj.new = fun [] do
  o = {}
  setmeta(o, Obj.meta)
  return o
end

let p = Obj.new()
p["whatever"]
